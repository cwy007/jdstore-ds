<!-- start of landing page页面轮播 -->
<div id="myCarousel" class="carousel slide welcome" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
    <li data-target="#myCarousel" data-slide-to="3"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src=" <%= image_url "pic-1.png" %> " alt="Los Angeles">
    </div>

    <div class="item">
      <img src="<%= image_url "pic-2.png" %>" alt="Chicago">
    </div>

    <div class="item">
      <img src="<%= image_url "pic-3.png" %>" alt="New York">
    </div>

    <div class="item">
      <img src="<%= image_url "pic-4.png" %>" alt="New York">
    </div>
  </div>

  <!-- Left and right controls -->
  <a class="left carousel-control" href="#myCarousel" data-slide="prev">
    <i class="fa fa-angle-left"></i>
  </a>
  <a class="right carousel-control" href="#myCarousel" data-slide="next">
    <i class="fa fa-angle-right"></i>
  </a>
</div>

<!-- end of landing page页面轮播 -->
